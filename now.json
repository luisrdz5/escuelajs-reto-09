{
    "name": "reto9",
    "version": 2,
    "builds": [{"src": "src/index.js", "use": "@now/node"}],
    "routes": [{"src": "/(.*)", "dest": "/src/index.js"}],
    "env": {
        "DB_USER": "@db-user",
        "DB_PASSWORD": "@db-password",
        "DB_HOST": "@db-host",
        "DB_NAME":"@db-name"
    }
    
}